# To source from the ~/.bashrc

# Go
export GOPATH=~/go
export PATH=~/go/bin:/usr/local/go/bin:~/.local/bin:~/.local/google-cloud-sdk/bin/:~/.dotfiles/bin:~/.local/flutter/bin:$PATH

# android
export ANDROID_HOME=~/.local/android-sdk
export ANDROID_SDK_ROOT=$ANDROID_HOME

export CDPATH=.:~/src:~/src/dagger/pkg/universe.dagger.io/:/media/dolanor/ingolemo/sync/cloudtop-gopath/src/github.com:$GOPATH/src/github.com:$GOPATH/src:$GOPATH/src/code.tuxago.com:$CDPATH
#
# Used by my vim bindings for code review
REVIEW_BASE=origin/master

export GOPHERJS_GOROOT=~/sdk/go1.12.16

# dagger don't export our own usage (employee)
export DO_NOT_TRACK=1
export DAGGER_LOG_FORMAT=plain
