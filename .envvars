# To source from the ~/.bashrc

# Go
export GOPATH=~/go
export PATH=~/go/bin:/usr/local/go/bin:~/.local/bin:~/.local/google-cloud-sdk/bin/:~/.dotfiles/diff-so-fancy/:~/.local/flutter/bin:$PATH

# android
export ANDROID_HOME=~/.local/android-sdk
export ANDROID_SDK_ROOT=$ANDROID_HOME

CDPATH=.:$GOPATH/src/github.com:$GOPATH/src:$GOPATH/src/code.tuxago.com
#
# Used by my vim bindings for code review
REVIEW_BASE=origin/master

export GOPHERJS_GOROOT=~/sdk/go1.12.16
